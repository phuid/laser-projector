#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../repl.history/bin/repl.history" "$@"
  ret=$?
else 
  node  "$basedir/../repl.history/bin/repl.history" "$@"
  ret=$?
fi
exit $ret
